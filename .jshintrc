{
  "bitwise": true,
  "camelcase": true,
  "curly": false,
  "enforceall": false,
  "eqeqeq": true,
  "esversion": 6,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxcomplexity": 4,
  "maxdepth": 4,
  "maxerr": 50,
  "maxlen": 80,
  "maxparams": 4,
  "maxstatements": 15,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "predef": [
    "describe",
    "it",
    "before"
  ],
  "singleGroups": true,
  "strict": true,
  "node": true,
  "undef": false,
  "unused": false,
  "shadow": false,
  "boss": false,
  "debug": false,
  "globals": {}
}
